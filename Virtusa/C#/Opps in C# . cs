using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace ConsoleApplication1
{
  class Test
  {
    private int X;
    private int Y;
    public Test()
    {
        X=5;
        Y=10;
    }
    public void read()
    {
        X=Convert.ToInt32(Console.ReadLine());
        Y=Convert.ToInt32(Console.ReadLine());
    }
    public void display()
    {
        Console.WriteLine("Updated Valkue of X = {0}",X);
        Console.WriteLine("Updated Valkue of Y = {0}",Y);
        
    }
  }
  class Program
  {
      static void Main(string [] args)
      {
          Test obj = new Test();
          obj.display();
          
          Test obj2 = new Test();
          obj2.read();
          obj2.display();
          
      }
  }
}
